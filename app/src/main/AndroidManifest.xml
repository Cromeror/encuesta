<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.crom.encuesta">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".view_controller.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view_controller.FamilyActivity"
            android:label="@string/title_activity_family"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view_controller.OcupadoActivity"
            android:label="@string/title_activity_ocupado"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".view_controller.ViviendaHogarActivity"
            android:label="@string/title_activity_vivienda_hogar"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>
